{
    "name": "renanonlinbe8/orc-project",
    "description": "A OCR converted",
    "keywords": ["ocr"],
    "homepage": "https://github.com/renanonline8/ocr-project",
    "license": "MIT",
    "authors": [
        {
            "name": "Renan Santos Gomes",
            "email": "support@renansgomes.com.br",
            "homepage": "http://www.renansgomes.com.br"
        }
    ],
    "require": {
        "php": ">=5.6",
        "monolog/monolog": "^1.17",
        "renanonline8/slim-utils": "1.*",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1",
        "thiagoalessio/tesseract_ocr": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "src/app/",
            "Utils\\": "src/utils/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
